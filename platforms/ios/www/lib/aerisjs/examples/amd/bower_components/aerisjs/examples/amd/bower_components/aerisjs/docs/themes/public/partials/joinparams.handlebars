{{#joinEach params ', '}}{{name}}:
{{#eachType type}}
  <a href="{{link}}" class="ref{{#if ../description}} description{{/if}}"
     title="<code>{{name}}</code>{{#if ../description}}<br>{{../../description}}{{/if}}"
  {{#if isNative}}
     target="_blank"
             >{{name}}</a>
  {{else}}
    >{{shortName name}}</a>
  {{/if}}
{{/eachType}}
{{/joinEach}}