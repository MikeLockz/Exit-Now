  <li class="category">
    <details>
      <summary><a href="#{{name}}">{{shortName name}}</a></summary>
      <ul class="tocSection">
        {{#each classes}}
          <li><a href="#{{name}}">{{shortName this}}</a></li>
        {{/each}}
        {{#each namespaces}}
          {{! Recursively render child namespaces }}
          {{>toc-classes this}}
        {{/each}}
      </ul>
    </details>
  </li>
